{
    "source": {
      "include": ["./frontend/src/", "./backend/server.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs|backend/routers)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "tutorials": "./tutorials"
    }
  }